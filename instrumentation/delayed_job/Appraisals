%w[7.1.0 7.2.0 8.0.0].each do |version|
  appraise "delayed_job_4.2-activejob-#{version}" do
    gem 'activejob', "~> #{version}"
    gem 'delayed_job', '~> 4.2'
  end
end

appraise 'delayed_job-latest' do
  gem 'activejob'
  gem 'delayed_job'
end
